{
  "extends": [
    "@rocketseat/eslint-config/next",
    "next/core-web-vitals",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "plugins": [
    "simple-import-sort",
    "@tanstack/query"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "@typescript-eslint/no-empty-object-type": "off",
    "camelcase": "off"
  }
}